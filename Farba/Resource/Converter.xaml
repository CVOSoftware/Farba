<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Farba.Resource.Converter"
    xmlns:local="clr-namespace:Farba.Resource">

    <c:BrushConverter x:Key="@Converter/BrushConverter" />
    <c:RGBConverter x:Key="@Converter/RGBConverter" />
    <c:HEXConverter x:Key="@Converter/HEXConverter" />
    <c:EnumToStringConverter x:Key="@Converter/EnumToStringConverter"/>
    <c:EnumCollectionConverter x:Key="@Converter/EnumCollectionConverter"/>
    <c:AdaptiveWidthConverter x:Key="@Converter/AdaptiveWidthConverter"/>
    <c:AdaptiveHeightConverter x:Key="@Converter/AdaptiveHeightConverter"/>
    <c:ColorCombinationTypeConveter x:Key="@Converter/ColorCombinationTypeConverter"/>
    <c:SwitchImageConverter x:Key="@Converter/SwitchSortImage" First="{StaticResource ResourceKey=@Image/Up}" Second="{StaticResource ResourceKey=@Image/Down}" />
    <c:SwitchImageConverter x:Key="@Converter/SwitchSquareImage" First="{StaticResource ResourceKey=@Image/Square}" Second="{StaticResource ResourceKey=@Image/SquareFill}" />
    <c:SwitchImageConverter x:Key="@Converter/SwitchCircleImage" First="{StaticResource ResourceKey=@Image/Circle}" Second="{StaticResource ResourceKey=@Image/CircleFill}" />

</ResourceDictionary>